#include<iostream>

using namespace std;

void selection_sort(int arr[],int n){

    int minindex;

    for(int i=0;i<n-1;i++){
        minindex=i;
        for(int j=i+1;j<n;j++){
            if(arr[j]<arr[minindex]){
                minindex=j;
            }
        }
        swap(arr[i],arr[minindex]);
    }
}

void bubble_sort(int arr[],int n){


    for(int i=1;i<n;i++){

        int notsorted=0;

        for(int j=0;j<n-i;j++){
            if(arr[j]>arr[j+1]){
                swap(arr[j],arr[j+1]);
                notsorted=1;
            }
        }
        if(!notsorted)break;
    }
}

void insertion_sort(int arr[],int n){
  
    for(int i=1;i<n;i++){
        
        int key=arr[i];
        int j=i-1;
        for(;j<=0;j--){
            if(arr[j]>key){
                arr[j+1]=key;
            }
            else{
                break;
            }
        }
        arr[j+1]=key;
    }
}

int main(){

    return 0;
}